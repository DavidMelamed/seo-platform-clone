# SEO Platform Environment Variables

# Application Settings
ENVIRONMENT=development
SECRET_KEY=your-secret-key-here-change-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=30
ALGORITHM=HS256

# Database URLs
DATABASE_URL=postgresql://seo_user:seo_password@localhost:5432/seo_platform
TIMESCALE_URL=postgresql://timescale_user:timescale_password@localhost:5433/seo_timeseries
REDIS_URL=redis://:redis_password@localhost:6379/0
CLICKHOUSE_URL=http://clickhouse_user:clickhouse_password@localhost:8123/seo_analytics
ELASTICSEARCH_URL=http://localhost:9200

# Kafka Settings
KAFKA_BOOTSTRAP_SERVERS=localhost:9092

# MinIO Object Storage
MINIO_ENDPOINT=localhost:9010
MINIO_ACCESS_KEY=minio_user
MINIO_SECRET_KEY=minio_password
MINIO_SECURE=false

# API Keys - IMPORTANT: Add your actual API keys here
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
DATAFORSEO_LOGIN=your-dataforseo-login
DATAFORSEO_PASSWORD=your-dataforseo-password
GOOGLE_API_KEY=your-google-api-key
PINECONE_API_KEY=your-pinecone-api-key
PINECONE_ENVIRONMENT=your-pinecone-environment

# Email Settings (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-email-password
EMAIL_FROM=noreply@seoplatform.com

# Monitoring
PROMETHEUS_PORT=9090
GRAFANA_PORT=3001

# Frontend Settings
REACT_APP_API_URL=http://localhost:8000/api/v1
REACT_APP_WS_URL=ws://localhost:8000

# Admin User (created on first run)
ADMIN_EMAIL=admin@seoplatform.com
ADMIN_PASSWORD=admin123
ADMIN_FULL_NAME=SEO Admin
ADMIN_ORGANIZATION=SEO Platform